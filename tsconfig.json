{
  "compilerOptions": {
    "outDir": "./dist/",
    "module": "es6",
    "target": "es5",
    "allowJs": true,
    "noImplicitAny": true,
    "noFallthroughCasesInSwitch": true,
    "strictNullChecks": true,
    "noImplicitThis": false,
    "noUnusedParameters": false,
    "noUnusedLocals": true,
    "noImplicitReturns": true,
    "pretty": true,
    "moduleResolution": "node",
    "jsx": "react",
    "types": [
      "classnames",
      "react",
      "react-redux",
      "react-dom",
      "jest"
    ]
  }
}
